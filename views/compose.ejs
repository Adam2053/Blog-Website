<%- include('./partials/header'); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <label for="blog-title">Title</label> <br>
    <input type="text" name="blog" id="blog-title" > <br>
    <label for="blog-content">Post</label> <br>
    <textarea name="content" id="blog-content" cols="30" rows="10"></textarea> <br>
    <button type="submit" class="btn" name="button">Publish</button>
</form>
<%- include('./partials/footer'); -%>